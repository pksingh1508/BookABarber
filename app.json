{
  "expo": {
    "name": "BookABarber",
    "slug": "BarberAppointment",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "imagewidth": 150,
      "resizeMode": "contain",
      "backgroundColor": "#7e9cad"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#7e9cad"
      },
      "package": "com.pawankumar1212.BarberAppointment",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCTiZAspHCRRqNiWc2iXQ8cztCiiMAPOoo"  
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-community/datetimepicker",
        {
          "android": {
            "timePicker": {
              "background": {
                "light": "#232323",
                "dark": "#232323"
              },
              "numbersBackgroundColor": {
                "light": "#FF7900",
                "dark": "#FF7900"
              }
            }
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow BookABarber to use your location."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b64dc762-3caf-428b-a294-780e5e757ef5"
      }
    }
  }
}
